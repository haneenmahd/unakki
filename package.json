{
  "name": "unakki",
  "version": "1.0.1",
  "description": "Boost your productivity by simplifying the process of organising your project ⚡️",
  "main": "bin/unakki.js",
  "type": "module",
  "author": "Haneen Mahdin<haneenmahdin@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": "dist/bin/unakki.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && yarn build:src && yarn build:bin",
    "build:src": "babel src -d dist/src",
    "build:bin": "babel bin -d dist/bin",
    "dev": "node --es-module-specifier-resolution=node .",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "typeinstall": "^2.1.0"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "inquirer": "^8.2.2"
  },
  "keywords": [
    "productivity",
    "organise",
    "nodejs",
    "projects",
    "tools",
    "developer"
  ]
}
